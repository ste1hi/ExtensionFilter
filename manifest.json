{
    "name": "ExtensionFilter",
    "description": "Filter Chrome extensions based on URL.",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["management", "storage", "tabs"],
    "icons": {
        "128": "img/filter.png"
    },
    "action": {
      "default_popup": "html/popup.html",
      "type": "module"
    },
    "background": {
      "service_worker": "scripts/background.js",
      "type": "module"
    },
    "options_page": "html/option.html"
}